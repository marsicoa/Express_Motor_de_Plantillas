table.table
  thead.thead-dark
    th Articulo
    th Precio
    th Miniatura
  tbody
    if productos
    each el in productos
        tr
        td #{el.title}
        td #{el.price}
        td
            img.medicine(src='https://cdn1.iconfinder.com/data/icons/medical-2-19/512/medical-healthcare-hospital-12-512.png')
        tr
    else
      td
      td No hay productos
      td
